	</scripts>

    <snippets>
        <snippet name="CompoundCardItem">
            <Panel class="CompoundCardItem">
                <Panel id="CardInfo"/>
                <Panel id="DragPanel" class="CardBackPanel" draggable="true">
                    <Panel class="CardBg"/>
                    <Panel class="CardBorder"/>
