        </snippet>
    </snippets>
    <Button class="PopupContainer">
        <Panel id="CompoundCard">
            <Panel class="PopupsBox">
                <Panel class="PopupsContainer">
                    <Panel class="PopupsBody">
                        <Panel id="CompoundCardList" />
                    </Panel>
                    <Panel class="ButtonGroup">
                        <Button id="ClosedPopupsBtn" class="dashboard_btn">
                            <Label text="ç„¯" />
                        </Button>
                    </Panel>
                </Panel>
            </Panel>
        </Panel>
        <Movie id="DuguaiMoviePanel" src="file://{resources}/videos/custom_game/duguai.webm" html="false"/>
    </Button>
