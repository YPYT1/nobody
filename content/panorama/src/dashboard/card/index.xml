<root>
	<styles>
		<include src="./styles.less" />
        <include src="./../../_styles/styles.less" />
	</styles>
	<scripts>
		<include src="./script.tsx" />
        <include src="./_popups.tsx" />
	</scripts>
    <snippets>
        <snippet name="CardNavRadioButton">
            <RadioButton group="CardNavRadioButton" text="{s:button_txt}"/>
        </snippet>
    </snippets>
	<Panel class="DashBoardPanel">

        <Panel class="BoardContainer">
            <Panel class="BoardHeader">
                <Panel class="BoardTypeImage"/>
                <Button id="DashboardClosedBtn" class="ClosedBtn" />
            </Panel>
            <Panel class="BoardContent">
                <Panel id="NavButtonList" class="NavButtonGroupHor">
                
                </Panel>
                <Panel id="ContentFrame" class="ContentFrame">

                </Panel>
            </Panel>

        </Panel>
        <Button id="Card_PopupsBg" class="PopupsBackground">
            <Panel id="PlayerConsumeCard" class="PopupsWindows">
                <Panel class="PopupsHeader">
                    <Label text="是否登记卡片"/>
                </Panel>
                <Panel class="ButtonGroup">
                    <Button id="BtnConfirm" class="dashboard_btn ">
                        <Label text="确认" />
                    </Button>
                    <Button id="ClosedPopupsBtn" class="dashboard_btn yellow">
                        <Label text="取消" />
                    </Button>
                </Panel>
                
            </Panel>

            <Panel id="CompoundCard" class="PopupsBox">
                <Panel id="CardList" class="PopupsBody">
                    <Panel class="CardItem" />
                    <Panel class="CardItem" />

                </Panel>
                <Panel class="PopupsFooter">
                    <Button id="ClosedPopupsBtn" class="dashboard_btn">
                        <Label text="焯" />
                    </Button>
                </Panel>
            </Panel>
        </Button>
	</Panel>
</root>