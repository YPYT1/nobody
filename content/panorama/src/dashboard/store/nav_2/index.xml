<root>
	<styles>
		<include src="./styles.less" />
		<include src="./../../../_styles/styles.less" />
	</styles>
	<scripts>
		<include src="./script.tsx" />
	</scripts>
	<snippets>
		<snippet name="StoreItem">
			<Panel class="StoreItem">
				<Button id="StorePurchaseBtn" class="StoreBorder">
					<Panel class="Limitation">
						<Panel class="LimitBackground"/>
						<Panel class="LabelContent">
							<Label text="{s:limit_type}限购"/>
							<Label text="{d:limit_count} / {d:limit_max}"/>
						</Panel>
					</Panel>
					<Panel class="StoreIcon">
						<Image id="StoreIcon" />
					</Panel>
					<Label class="GoodsName"  text="{s:goods_name}"/>

					<Panel class="StorePriceContent">
						<Panel class="CurrencyIcon">
							<Image id="CurrencyIcon"  />
						</Panel>
						<Panel id="OriginalPanel" class="PriceContent Original">
							<Panel class="DiagonalCost">
								<Label text="{s:price_cost}"/>
								<Panel class="DiagonalShape"/>
							</Panel>
						</Panel>
						<Panel id="ActualPanel" class="PriceContent Actual">
							<Label text="{s:price_cost}"/>
						</Panel>
					</Panel>
					<Panel class="StorePurchased">
						<Label text="已购买"/>
					</Panel>
				</Button>

				<Panel class="HotTag"/>
				<Panel class="DiscountTag">
					<Label text="{d:discount}折"/>
				</Panel>
			</Panel>
		</snippet>
	</snippets>
	<Panel class="nav_content">
        <Panel class="NavContentFrame" hittest="false">
            <Panel class="VirtualNavButton" hittest="false"/>
            <Panel class="NavContainer" hittest="false">
				<Panel class="Container">
					<Panel id="MemberProductsList" />
				</Panel>
				
            </Panel>
        </Panel>
	</Panel>
</root>