    <styles>
        <include src="./../../_styles/keyframes.less" />
        <include src="./../../_styles/styles.less" />
        <include src="./../../_styles/components/tooltip.less" />
        <include src="./styles.less" />
    </styles>
    <Panel class="tooltip_prop tooltip_rare" hittest="false">
        <Panel class="Background">
            <Panel class="Header">
                <Panel class="Top"/>
